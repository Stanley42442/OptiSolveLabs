{
  "version": 2,
  "builds": [
    { "src": "dist/index.js", "use": "@vercel/node" },
    { "src": "dist/public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/index.js" }
  ],
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
